<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".DashboardActivity">



    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@drawable/tool_img"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Dark"
        android:elevation="4dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="60dp"
        android:background="@color/circleBorder" />


    <Button
        android:id="@+id/childhoodBtn"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/lpass_curves"
        android:text="Short Spaned innocent time"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="135dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="25dp"
        android:background="@color/circleBorder" />

    <Button
        android:id="@+id/hungerBtn"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/lpass_curves"
        android:text="Hunger Queen"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="133dp" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="25dp"
        android:background="@color/circleBorder" />

    <Button
        android:id="@+id/snapsBtn"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/lpass_curves"
        android:text="Bedtime Blues"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="230dp" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="25dp"
        android:background="@color/circleBorder" />

    <Button
        android:id="@+id/weBtn"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/lpass_curves"
        android:text="Humara FeviBond"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="248dp" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="25dp"
        android:background="@color/circleBorder" />

    <Button
        android:id="@+id/youBtn"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/lpass_curves"
        android:text="My mischeif minion"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="354dp" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="25dp"
        android:background="@color/circleBorder" />

    <Button
        android:id="@+id/memoryBtn"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/lpass_curves"
        android:text="A sweet memory"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="354dp" />



</LinearLayout>